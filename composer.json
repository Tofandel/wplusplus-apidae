{
  "name": "tofandel/wplusplus-apidae",
  "version": "1.9.3",
  "authors": [
    {
      "name": "Tofandel"
    }
  ],
  "type": "wordpress-plugin",
  "description": "W++ apidae allows you to use apidae with wordpress simply by creating Twig templates",
  "license": "Apache 2.0",
  "keywords": [
    "plugin",
    "wordpress",
    "apidae",
    "twig"
  ],
  "support": {
    "issues": "https://github.com/tofandel/wplusplus-apidae/issues"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=7.0",
    "composer/installers": "~1.0",
    "tgmpa/tgm-plugin-activation": ">=2.6",
    "twig/twig": ">=2.0"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Tofandel\\Apidae\\": "src/"
    }
  }
}